#: marketpress-includes/marketpress-ms.php:198
#@ mp
msgid "Global Products List"
msgstr "Daftar Produk Global"

#: marketpress-includes/marketpress-ms.php:201
#@ mp
msgid "Displays a network-wide list of products according to preference."
msgstr "Tampilkan daftar panjang jaringan sesuai pilihan"

#: marketpress-includes/marketpress-ms.php:203
#: marketpress-includes/marketpress-ms.php:228
#: marketpress-includes/marketpress-ms.php:243
#: marketpress.php:5253
#: marketpress.php:5266
#: marketpress.php:5291
#@ mp
msgid "Optional Attributes:"
msgstr "Atribut Pilihan:"

#: marketpress-includes/marketpress-ms.php:205
#@ mp
msgid "\"paginate\" - Whether to paginate the product list. This is useful to only show a subset. Default: 1"
msgstr "\"paginasi\" - Apakah memberi halaman daftar produk. Ini berguna hanya untuk menampilkan sebagian. Standar: 1"

#: marketpress-includes/marketpress-ms.php:206
#@ mp
msgid "\"page\" - How many products to display in the product list if \"paginate\" is set to true. Default: 20"
msgstr "\"page\" - Berapa produk untuk ditampilkan dalam daftar produk jika \"paginasi\" pengaturan benar. Standar: 20"

#: marketpress-includes/marketpress-ms.php:207
#@ mp
msgid "\"per_page\" - How many products to display in the product list if \"paginate\" is set to 1."
msgstr "\"per_page\" - Berapa produk ditampilkan dalam daftar produk jika \"paginasi\" pengaturan 1."

#: marketpress-includes/marketpress-ms.php:208
#@ mp
msgid "\"order_by\" - What field to order products by. Can be: date, title, price, sales, rand. Default: date"
msgstr "\"order_by\" - Apakah kolom berdasarkan pesanan produk. Bisa: tanggal, judul, harga, penjualan, acak. Standar: tanggal"

#: marketpress-includes/marketpress-ms.php:209
#: marketpress-includes/marketpress-ms.php:247
#@ mp
msgid "\"order\" - Direction to order products by. Can be: DESC, ASC. Default: DESC"
msgstr "\"order\" - Arahan berdasarkan pesanan produk. Bisa DESC, ASC. Standar: DESC"

#: marketpress-includes/marketpress-ms.php:210
#: marketpress.php:5273
#@ mp
msgid "\"category\" - Limits list to a specific product category. Use the category Slug"
msgstr "\"kategori\" - Batas daftar berdasarkan kategori produk khusus. Gunakan Slug kategori"

#: marketpress-includes/marketpress-ms.php:211
#: marketpress.php:5274
#@ mp
msgid "\"tag\" - Limits list to a specific product tag. Use the tag Slug"
msgstr "\"lebel\" - Batas daftar label produk khusus. Gunakan Slug label"

#: marketpress-includes/marketpress-ms.php:212
#@ mp
msgid "\"show_thumbnail\" - Whether to show the product thumbnail. Default: 1"
msgstr "\"show_thumbnail\" - Apakah menampilkan thumbnail produk. Standar: 1"

#: marketpress-includes/marketpress-ms.php:213
#@ mp
msgid "\"thumbnail_size\" - Max thumbnail width/height. Default: 150"
msgstr "\"thumbnail_size\" - Maks thumbnail lebar/tinggi. Standar: 150"

#: marketpress-includes/marketpress-ms.php:214
#@ mp
msgid "\"show_price\" - Whether to show the product price. Default: 1"
msgstr "\"show_price\" - Apakah menampilkan harga produk. Standar: 1"

#: marketpress-includes/marketpress-ms.php:215
#@ mp
msgid "\"text\" - Choose \"excerpt\", \"content\", or \"none\". Default: excerpt"
msgstr "\"text\" - Pilih \"excerpt\", atau \"none\". Standar: excerpt"

#: marketpress-includes/marketpress-ms.php:216
#@ mp
msgid "\"as_list\" - Whether to show as an unordered list. Default: 0"
msgstr "\"as_list\" - Apakah menampilkan sebagai daftar belum dipesan. Standar: 0 "

